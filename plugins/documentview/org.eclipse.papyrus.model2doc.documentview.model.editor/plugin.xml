<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated documentView -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.papyrus.model2doc.documentview.presentation.DocumentViewModelWizardID"
            name="%_UI_DocumentViewModelWizard_label"
            class="org.eclipse.papyrus.model2doc.documentview.presentation.DocumentViewModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DocumentViewModelFile.gif">
         <description>%_UI_DocumentViewModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated documentView -->
      <editor
            id="org.eclipse.papyrus.model2doc.documentview.presentation.DocumentViewEditorID"
            name="%_UI_DocumentViewEditor_label"
            icon="icons/full/obj16/DocumentViewModelFile.gif"
            extensions="documentview"
            class="org.eclipse.papyrus.model2doc.documentview.presentation.DocumentViewEditor"
            contributorClass="org.eclipse.papyrus.model2doc.documentview.presentation.DocumentViewActionBarContributor">
      </editor>
   </extension>

</plugin>
