<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup?after=org.eclipse.papyrus.views.modelexplorer.popupmenu.creatediagram">
         <menu
               icon="platform:/plugin/org.eclipse.papyrus.model2doc.documentview.ui/resources/icon/new_document.png"
               id="org.eclipse.papyrus.model2doc.ui.popupmenu.createdocument"
               label="New Document"
               tooltip="The Papyrus Document Menu">
            <dynamic
                  class="org.eclipse.papyrus.model2doc.documentview.ui.internal.menu.dynamiccontribution.DynamicPapyrusDocumentViewsMenuContribution"
                  id="org.eclipse.papyrus.model2doc.documentview.ui.menu.dynamiccontribution.PapyrusDocumentViewsMenu">
            </dynamic>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.papyrus.infra.viewpoints.policy.viewType">
      <helper
            class="org.eclipse.papyrus.model2doc.documentview.ui.internal.viewpoint.helper.CreatePapyrusDocumentViewTypeCommandHelper"
            priority="0">
      </helper>
   </extension>
<extension
       point="org.eclipse.papyrus.infra.ui.papyrusDiagram">
    <editorDiagram
          factoryClass="org.eclipse.papyrus.model2doc.documentview.ui.internal.editor.DocumentViewEditorFactory"
          icon="resources/icon/new_document.png">
    </editorDiagram>
 </extension>
 <extension
      point="org.eclipse.core.runtime.adapters">
   <factory
         adaptableType="org.eclipse.papyrus.model2doc.documentview.Document"
         class="org.eclipse.papyrus.model2doc.documentview.ui.internal.adapter.DocumentViewAdapterFactory">
      <adapter
            type="org.eclipse.papyrus.infra.core.sasheditor.di.contentprovider.IOpenable">
      </adapter>
   </factory>
</extension>
</plugin>
