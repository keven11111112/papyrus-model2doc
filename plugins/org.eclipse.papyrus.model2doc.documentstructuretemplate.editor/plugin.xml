<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated documentstructuretemplate -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.papyrus.model2doc.documentstructuretemplate.presentation.DocumentStructureTemplateModelWizardID"
            name="%_UI_DocumentStructureTemplateModelWizard_label"
            class="org.eclipse.papyrus.model2doc.documentstructuretemplate.presentation.DocumentStructureTemplateModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DocumentStructureTemplateModelFile.gif">
         <description>%_UI_DocumentStructureTemplateModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated documentstructuretemplate -->
      <editor
            id="org.eclipse.papyrus.model2doc.documentstructuretemplate.presentation.DocumentStructureTemplateEditorID"
            name="%_UI_DocumentStructureTemplateEditor_label"
            icon="icons/full/obj16/DocumentStructureTemplateModelFile.gif"
            extensions="documentstructuretemplate"
            class="org.eclipse.papyrus.model2doc.documentstructuretemplate.presentation.DocumentStructureTemplateEditor"
            contributorClass="org.eclipse.papyrus.model2doc.documentstructuretemplate.presentation.DocumentStructureTemplateActionBarContributor">
      </editor>
   </extension>

</plugin>
